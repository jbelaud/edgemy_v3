{
  "name": "shadcn-ui-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "pnpm --filter=api prisma generate && turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean",
    "test": "turbo test",
    "check-types": "turbo check-types",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.1",
    "turbo": "^2.5.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
