{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "postinstall": "pnpm prisma:generate",
    "build": "next build",
    "vercel-build": "node build-vercel.js",
    "vercel-build-with-migrations": "prisma generate --schema=../../packages/prisma/schema.prisma --generator=client_web && prisma migrate deploy --schema=../../packages/prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "postbuild": "next-sitemap",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate --schema=../../packages/prisma/schema.prisma --generator=client_web",
    "prisma:migrate": "prisma migrate dev --schema=../../packages/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=../../packages/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=../../packages/prisma/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.13",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-table": "^8.21.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@workspace/ui": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.10.1",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "limiter": "^3.0.0",
    "lucide-react": "^0.475.0",
    "next": "^15.2.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@workspace/typescript-config": "file:../../packages/typescript-config",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.0",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3"
  }
}
