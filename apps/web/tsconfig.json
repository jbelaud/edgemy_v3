{
  "extends": "@workspace/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@workspace/ui/*": ["../../packages/ui/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "next-env.d.ts",
    "next.config.ts",
    "types/**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "../../packages/ui/src/components/ui/button.tsx", "../../packages/ui/src/components/ui/card.tsx", "../../packages/ui/src/components/ui/checkbox.tsx", "../../packages/ui/src/components/ui/dialog.tsx", "../../packages/ui/src/components/ui/dropdown-menu.tsx", "../../packages/ui/src/components/ui/input.tsx", "../../packages/ui/src/components/ui/label.tsx", "../../packages/ui/src/components/ui/login-form.tsx", "../../packages/ui/src/components/ui/textarea.tsx"  ],
  "exclude": ["node_modules"]
}
